<!DOCTYPE html>
<html>
<head>
  <title>{{ title }}</title>
  <script disable-src="app.js"></script>
</head>
<body>
  {{> header }}
  <h1>{{ headline }}</h1>
  <h1>{{ articleTitle }}</h1>
  <ul class="articles">
    {{#each articles}}
    <li>{{this.name}}: {{this.price}}</li>
    {{/each}}
  </ul>
  {{> footer }}
</body>
</html>
